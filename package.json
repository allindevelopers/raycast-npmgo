{
	"$schema": "https://www.raycast.com/schemas/extension.json",
	"name": "npmgo",
	"title": "NPM GO",
	"description": "Allows to jump to different websites related to a NPM Package.",
	"icon": "command-icon.png",
	"author": "iamandrewluca",
	"categories": [
		"Web",
		"Productivity",
		"Developer Tools",
		"Documentation"
	],
	"license": "MIT",
	"commands": [
		{
			"name": "index",
			"title": "Show Typeahead Results",
			"description": "Template for showing typeahead search with network results.",
			"mode": "view"
		}
	],
	"dependencies": {
		"@allindevelopers/npmgo": "^1.1.1",
		"@raycast/api": "^1.64.4",
		"@raycast/utils": "^1.4.8",
		"fuse.js": "^7.0.0"
	},
	"devDependencies": {
		"@allindevelopers/prettier-config": "^1.0.0",
		"@raycast/eslint-config": "^1.0.6",
		"@types/node": "20.8.10",
		"@types/react": "18.2.27",
		"eslint": "^8.51.0",
		"prettier": "^3.1.1",
		"typescript": "^5.2.2"
	},
	"scripts": {
		"build": "ray build -e dist",
		"dev": "ray develop",
		"fix-lint": "ray lint --fix",
		"lint": "ray lint",
		"publish": "npx @raycast/api@latest publish"
	},
	"prettier": "@allindevelopers/prettier-config"
}
